<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.landleaf.homeauto.center.device.model.mapper.FamilyTerminalMapper">


    <select id="getListByFamilyId"
            resultType="com.landleaf.homeauto.center.device.model.vo.family.FamilyTerminalPageVO">
      select t.id,t.name,t.mac,t.master_flag,t.type from family_terminal t where t.family_id = #{familyId} order by t.create_time asc
    </select>
</mapper>
