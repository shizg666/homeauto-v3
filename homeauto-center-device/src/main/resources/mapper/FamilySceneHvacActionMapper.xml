<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.landleaf.homeauto.center.device.model.mapper.FamilySceneHvacConfigActionMapper">

    <select id="getListIds" resultType="java.lang.String">
        select id from family_scene_hvac_action ha
        where ha.hvac_config_id in
        <foreach collection="hvacConfigIds" open="(" close=")" separator="," item="item">
            #{item}
        </foreach>
    </select>
</mapper>
